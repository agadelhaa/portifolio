<p id="notice"><%= notice %></p>

<h1> Portifolio Amanda </h1>

<div>
  <%= link_to 'Novo Projeto', new_project_path %>
  <table>
    <thead>
      <tr>
        <th> Nome </th>
        <th> Categoria </th>
      </tr>
    </thead>

    <tbody>
      <% @projects.each do |project| %>
        <tr>
          <td><%= project.name %></td>
          <td><%= project.category %></td>
          <td><%= link_to 'Mostrar', project %></td>
        
      <% end %>
  </table>
</div>
